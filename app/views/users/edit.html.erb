<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

  <%= form_for @user do |f| %>
    <%= f.label :fname, "First name: " %>
    <%= f.text_field :fname %>
    <br>
    <%= f.label :lname, "Last name: " %>
    <%= f.text_field :lname %>
    <br>
    <%= f.label :username, "Username: " %>
    <%= f.text_field :username %>
    <br>
    <%= f.submit %>
  <% end %>
